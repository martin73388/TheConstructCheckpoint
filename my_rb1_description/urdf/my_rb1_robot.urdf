<?xml version="1.0" ?>

<robot name="robot">

        <!-- Gazebo tags - Laser scan -->
    <gazebo reference="joint_laser_scan_chassis">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="link_laser_scan">
        <material>Gazebo/DarkGrey</material>
    </gazebo>

    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>







    <!-- Link - base -->
    <link name="base_link">
        <inertial>
            <mass value="25" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.578125" ixy="0" ixz="0" iyy="0.578125" iyz="0" izz="0.78125" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.3" radius="0.25"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder length="0.3" radius="0.25"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 0.5" />
            </material>
        </visual>
    </link>

    <!-- Joint - base / footprint-->
    <joint name="joint_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 -0.15" />
        <child link="base_footprint" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 0 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - footprint -->
    <link name="base_footprint">
        <inertial>
            <mass value="0" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="1.5707" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>
    </link>

    <!-- Joint - base / right_wheel-->
    <joint name="joint_base_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - right wheel -->
    <link name="right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <!-- wheel weight : 1kg -->
            <inertia ixx="0.00203125" ixy="0" ixz="0" iyy="0.0003125" iyz="0" izz="0.00203125"/>
        </inertial>

        <!-- right wheel -->
        <collision>
            <origin rpy="1.5707 0 0 " xyz="0 0 0" />
            <geometry>
                <cylinder length="0.15" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.15" radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="0.7 0.1 0.1 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - base / left_wheel-->
    <joint name="joint_base_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - left wheel -->
    <link name="left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
             <!-- wheel weight : 1kg -->
            <inertia ixx="0.00203125" ixy="0" ixz="0" iyy="0.0003125" iyz="0" izz="0.00203125"/>
        </inertial>

        <!-- left wheel -->
        <collision>
            <origin rpy="1.5707 0 0 " xyz="0 0 0" />
            <geometry>
                <cylinder length="0.15" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.15" radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="0.7 0.1 0.1 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - base / front_caster-->
    <joint name="joint_base_front_caster" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0 -0.15" />
        <child link="front_caster" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="0" />
    </joint>
    <!-- Link - front_caster -->
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
             <!-- caster weight : 0.2kg -->
            <inertia ixx="0.00005" ixy="0" ixz="0" iyy="0.00005" iyz="0" izz="0.00005"/>
        </inertial>

        <!-- front_caster -->
        <collision>
            <origin rpy="0 0 0 " xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - base / back_caster-->
    <joint name="joint_base_back_caster" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0 -0.15" />
        <child link="back_caster" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="0" />
    </joint>
    <!-- Link - back_caster -->
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
             <!-- caster weight : 0.2kg -->
            <inertia ixx="0.00005" ixy="0" ixz="0" iyy="0.00005" iyz="0" izz="0.00005"/>
        </inertial>

        <!-- back_caster -->
        <collision>
            <origin rpy="0 0 0 " xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- Front Laser -->
    <joint name="joint_base_laser" type="fixed">
        <origin rpy="3.1415 0 0" xyz="0.25 0 0.075" />
        <child link="front_laser" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.5" />
            <inertia ixx="0.000252666666667" ixy="0" ixz="0" iyy="0.000252666666667" iyz="0" izz="0.0005"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.015" length="0.020"/>
            </geometry>
            <material name="Red">
                <color rgba="0.4 0.4 0.4 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.015" length="0.020"/>
            </geometry>
        </collision>
    </link>

</robot>
